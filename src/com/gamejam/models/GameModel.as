package com.gamejam.models{	/* 	* Stores game data 	*/		public class GameModel	{			private static var _instance:GameModel;				private static var _timeForPlayerToDie:Number = 110;				private var _started : Boolean = false;		private var _paused : Boolean = false;		private var _finished : Boolean = false;			public function GameModel()		{			if (_instance) throw new Error("GameModel.GameModel: ERROR: Instantiation failed: Use GameView.getInstance()");					}				public static function getInstance():GameModel		{			if (_instance == null) _instance = new GameModel();			return _instance;		}				static public function get timeForPlayerToDie():Number { return _timeForPlayerToDie; }				public function get paused():Boolean 		{			return _paused;		}				public function set paused(value:Boolean):void 		{			_paused = value;		}				public function get started():Boolean 		{			return _started;		}				public function set started(value:Boolean):void 		{			_started = value;		}				public function get finished():Boolean 		{			return _finished;		}				public function set finished(value:Boolean):void 		{			_finished = value;		}	}}