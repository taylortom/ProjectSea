package com.gamejam.display{	import com.gamejam.managers.CollisionManager;	import com.gamejam.display.GameObject;		/**	 * Object which affects player's temperature	 * @author Kev Adsett	 */	public class TemperatureObject extends GameObject	{		private var _isHot: Boolean;				public function TemperatureObject() 		{			this.collideable = true;			CollisionManager.getInstance().addObject(this);		}				public function get isHot():Boolean { return _isHot; }		public function set isHot(value:Boolean):void { _isHot = value; }			}}