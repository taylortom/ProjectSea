package com.gamejam.display{	// Flash imports	import flash.display.MovieClip;	import flash.events.Event;	import com.gamejam.display.GameObject;	import com.gamejam.views.GameView;	/*	 * A Cloud... 	 */	public class Cloud extends GameObject	{		// in pixels per second		var _movementSpeed:Number;		var _movementDirection:Number;				public function Cloud()		{			super();						this._movementSpeed = Math.random()*10 + 25;			this._movementDirection = Math.round(Math.random());									this.moveable = true;		}				override protected function _move():void		{			if(this._movementDirection == 0) this.position.x -= (this._movementSpeed/30);			else this.position.x += (this._movementSpeed/30);		}	}}